<SCRIPT>
    function add(a,b)
    {
        return a+b;
    }
   
 add(20,10);    //this results in 30

    //bind function createas a copy of the function
    var addTen=add.bind(null,10)
    addTen(50);   //this results in 60


</SCRIPT>
